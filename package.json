    "docs:api": "jsdoc -c jsdoc.json",
    "docs:all": "npm run docs:commands && npm run docs:api",
    "docs:serve": "npx http-server docs/api -p 8080",
    "types:generate": "tsc",
    "types:check": "tsc --noEmit"
